{
  "name": "back-office",
  "version": "0.0.0",
  "description": "Ats Back Office",
  "main": "src/js/app.js",
  "scripts": {
    "clean": "rm -rf node_modules && npm cache clean",
    "install-app": "npm run install-bower && npm run install-gulp && npm run install-deps && npm run install-compass",
    "install-bower": "sudo npm install bower -g",
    "install-gulp": "sudo npm install gulp -g",
    "install-deps": "npm install && bower install",
    "install-compass": "gem update --system && gem install compass",
    "install-compass-fix": "rvm osx-ssl-certs update all"
  },
  "author": "James Hill",
  "license": "MIT",
  "devDependencies": {
    "async": "^0.8.0",
    "bower-resolve": "^1.1.0",
    "browserify": "^4.1.2",
    "browserify-shim": "^3.5.0",
    "chai": "^1.9.1",
    "chalk": "^0.4.0",
    "connect": "^2.15.0",
    "connect-livereload": "^0.4.0",
    "deamdify": "^0.1.1",
    "debowerify": "^0.7.0",
    "express": "^4.1.1",
    "findup-sync": "^0.1.3",
    "globule": "^0.2.0",
    "gulp": "^3.6.2",
    "gulp-autoprefixer": "0.0.7",
    "gulp-bower": "0.0.4",
    "gulp-bower-files": "^0.2.3",
    "gulp-bower-src": "0.0.1",
    "gulp-browserify": "^0.5.0",
    "gulp-clean": "^0.2.4",
    "gulp-commonjs": "^0.1.0",
    "gulp-compass": "^1.1.8",
    "gulp-concat": "^2.2.0",
    "gulp-csso": "^0.2.8",
    "gulp-filter": "^0.4.1",
    "gulp-if": "^1.1.0",
    "gulp-imagemin": "^0.5.0",
    "gulp-inject": "^0.4.1",
    "gulp-jshint": "^1.5.5",
    "gulp-karma": "0.0.4",
    "gulp-livereload": "^1.3.1",
    "gulp-load-plugins": "^0.5.0",
    "gulp-minify-html": "^0.1.3",
    "gulp-open": "^0.2.8",
    "gulp-plumber": "^0.6.1",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^0.3.2",
    "gulp-rimraf": "^0.1.0",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^0.7.1",
    "gulp-size": "^0.3.0",
