    "vinyl-source-stream": "^0.1.1",
    "watchify": "^0.8.2"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.min.js",
    "jquery-pubsub": "./bower_components/jquery-pubsub/jquery.pubsub.js",
    "underscore": "./bower_components/underscore/underscore.js",
    "backbone": "./bower_components/backbone/backbone.js",
    "marionette": "./bower_components/marionette/lib/backbone.marionette.js",
    "backbone.babysitter": "./bower_components/backbone.babysitter/lib/backbone.babysitter.js",
    "backbone.wreqr": "./bower_components/backbone.wreqr/lib/backbone.wreqr.js",
    "w2ui": "./bower_components/w2ui/w2ui-1.3.min.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "global:_",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "underscore:_"
      ]
    },
    "marionette": {
      "exports": "Marionette",
      "depends": [
        "backbone:Backbone"
      ]
    }
  },
  "browserify": {
    "transform": [
      "partialify",
      "browserify-shim"
    ]
  }
}
