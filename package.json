    "browserify-shim": {
        "./bower_components/jquery/dist/jquery.js": "$",
        "./bower_components/underscore/underscore.js": "_",
        "./bower_components/backbone/backbone.js": "Backbone",
        "./bower_components/marionette/backbone.marionette.js": "Marionette"
    },
    "browserify": {
        "transform": [
            "deamdify",
            "partialify",
            "browserify-shim"
        ]
    },
    "watchify": {
        "transform": [
            "deamdify",
            "partialify"
        ]
    },
    "jshintConfig": {
        "asi": false
    }
