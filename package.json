        "deamdify": "^0.1.1",
        "debowerify": "^0.7.0",
        "express": "^4.1.1",
        "gulp": "^3.6.2",
        "gulp-autoprefixer": "0.0.7",
        "gulp-bower": "0.0.4",
        "gulp-bower-files": "^0.2.3",
        "gulp-clean": "^0.2.4",
        "gulp-compass": "^1.1.8",
        "gulp-concat": "^2.2.0",
        "gulp-csso": "^0.2.8",
        "gulp-filter": "^0.4.1",
        "gulp-if": "^1.1.0",
        "gulp-imagemin": "^0.5.0",
        "gulp-inject": "^0.4.1",
        "gulp-jshint": "^1.5.5",
        "gulp-karma": "0.0.4",
        "gulp-livereload": "^1.3.1",
        "gulp-load-plugins": "^0.5.0",
        "gulp-minify-html": "^0.1.3",
        "gulp-open": "^0.2.8",
        "gulp-plumber": "^0.6.1",
        "gulp-rename": "^1.2.0",
        "gulp-rev": "^0.3.2",
        "gulp-rimraf": "^0.1.0",
        "gulp-run-sequence": "^0.3.2",
        "gulp-sass": "^0.7.1",
        "gulp-size": "^0.3.0",
        "gulp-streamify": "0.0.5",
        "gulp-uglify": "^0.2.1",
        "gulp-util": "^2.2.14",
        "gulp-watch": "^0.6.4",
        "http": "0.0.0",
        "jshint-stylish": "^0.2.0",
